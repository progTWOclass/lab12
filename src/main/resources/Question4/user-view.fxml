   <children>
      <TextField fx:id="nameText" layoutX="175.0" layoutY="105.0" prefWidth="250.0" promptText="Name" />
      <TextField fx:id="idText" layoutX="175.0" layoutY="142.0" prefWidth="250.0" promptText="ID" />
      <TextField fx:id="searchID" layoutX="175.0" layoutY="231.0" prefWidth="250.0" promptText="Enter ID to Search" />
      <Button fx:id="addButton" layoutX="175.0" layoutY="180.0" mnemonicParsing="false" onAction="#addEmployee" text="Add Employee" />
      <Button fx:id="searchButton" layoutX="175.0" layoutY="270.0" mnemonicParsing="false" onAction="#searchButton" text="Search by ID" />
